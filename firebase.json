{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
    "functions": {
      "source": "functions"
    }
  },
  "hosting": {
    "public": "static",
    "ignore": [
      "firebase.json"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "app"
      }
    ]
  }
}
